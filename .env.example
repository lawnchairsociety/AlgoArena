# API Port (defaults to 3000)
PORT=3000

# Market data provider (alpaca)
MARKET_DATA_PROVIDER=alpaca

# Alpaca (required when MARKET_DATA_PROVIDER=alpaca)
ALPACA_API_URL="https://paper-api.alpaca.markets/v2"
ALPACA_API_KEY=your_alpaca_api_key
ALPACA_API_SECRET=your_alpaca_api_secret

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/algoarena?sslmode=disable

# Redis/Valkey
REDIS_URL=redis://localhost:6379

# Master Key (for admin operations like creating API keys)
MASTER_KEY=your_master_key_here

# Resend (email notifications for API key requests)
RESEND_API_KEY=re_your_resend_api_key
RESEND_FROM_EMAIL=noreply@yourdomain.com
OWNER_EMAIL=owner@yourdomain.com
